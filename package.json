{
    "name": "asp-classic-with-jscript",
    "displayName": "ASP Classic with JScript",
    "description": "Language support for ASP classic using JScript",
    "version": "1.3.2",
    "repository": {
        "type": "git",
        "url": "https://github.com/Iislsdum/asp-classic-with-jscript.git"
    },
    "publisher": "darandos",
    "engines": {
        "vscode": ">=0.9.1"
    },
    "license": "GPL-3.0-or-later",
    "categories": [
        "Programming Languages"
    ],
    "keywords": [
        "Classic ASP",
        "Active Server Pages",
        "JavaScript"
    ],
    "contributes": {
        "languages": [
            {
                "id": "asp",
                "aliases": [
                    "ASP",
                    "asp"
                ],
                "extensions": [
                    ".asp",
                    ".inc"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "asp",
                "scopeName": "text.html.asp",
                "path": "./syntaxes/asp.tmLanguage.json",
                "embeddedLanguages": {
                    "source.asp": "asp",
                    "text.html": "html",
                    "source.css": "css",
                    "source.js": "javascript"
                }
            }
        ]
    }
}